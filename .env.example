# Intraday Trading Engine CLI defaults
# Copy to .env and adjust per environment.

# trade_engine.main orchestrator CLI
TRADE_ENGINE_MAIN=python -m trade_engine.main
TRADE_ENGINE_MAIN__CONFIG=config/engine.yaml
TRADE_ENGINE_MAIN__ENV_FILE=.env
TRADE_ENGINE_MAIN__DATABASE=trade_engine.db
TRADE_ENGINE_MAIN__TOP_N=20
TRADE_ENGINE_MAIN__CADENCE_MIN=5
TRADE_ENGINE_MAIN__LIVE=false

# trade_engine.tools.seed_watchlist helper
TRADE_ENGINE_SEED_WATCHLIST=python -m trade_engine.tools.seed_watchlist
TRADE_ENGINE_SEED_WATCHLIST__DATABASE=trade_engine.db
TRADE_ENGINE_SEED_WATCHLIST__FILE=symbols.txt

# trade_engine.tools.export helper
TRADE_ENGINE_EXPORT=python -m trade_engine.tools.export
TRADE_ENGINE_EXPORT__DATABASE=trade_engine.db
TRADE_ENGINE_EXPORT__OUT=export.json
